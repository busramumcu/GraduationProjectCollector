
@{
    ViewBag.Title = "GorevDeneme";
    Layout = "~/Views/Shared/_LoggedOgrenciLayout.cshtml";
}

<div class="panel panel-info">
    <div class="panel-heading">
        Compose a Message
    </div>
    <div class="panel-body">
        <form role="form">
            <div class="form-group">
                <label>Gorev Adı</label>
                <input class="form-control" type="text" />
            </div>
            <div class="form-group">
                <label>Açıklama</label>
                <input class="form-control" type="text" />
            </div>

            <div class="form-group">
                <label>Dosya Ekleyiniz </label>
                <input type="file" />
            </div>
            <button type="submit" class="btn btn-success">İşlemi Tamamla </button>           
        </form>
    </div>
</div>

