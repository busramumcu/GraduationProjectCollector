<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <!--[if IE]>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <![endif]-->
    <title>ANA SAYFA</title>
    <!-- BOOTSTRAP CORE STYLE  -->
    <link href="~/Content/Base_Page/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FONT AWESOME STYLE  -->
    <link href="~/Content/Base_Page/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- CUSTOM STYLE  -->
    <link href="~/Content/Base_Page/assets/css/style.css" rel="stylesheet" />
    <!-- GOOGLE FONT -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
    
    <link rel="Shortcut Icon" href="~/Content/Images/icon.ico" type="image/x-icon" />

</head>
<body>
    <div class="navbar navbar-inverse set-radius-zero">
        <div class="mycontainer">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="@Url.Action("Index", "Home")">

                        <img src="~/Content/Images/logo.png" height="60" />
                    </a>
                </div>

                <div class="right-div">
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
    </div>
    <!-- LOGO HEADER END-->
    <section class="menu-section">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="navbar-collapse collapse ">
                        <ul id="menu-top" class="nav navbar-nav navbar-right">
                            <li><a href="@Url.Action("Index", "Home")">ANASAYFA</a></li>
                            <li>
                                <a href="#" class="dropdown-toggle" id="ddlmenuItem" data-toggle="dropdown">ÖĞRETMENLER<i class="fa fa-angle-down"></i></a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="ddlmenuItem">
                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("TezKoordinatoru", "Home")">TEZ KOORDİNATÖRÜ</a></li>
                                    <li role="presentation"><a role="menuitem" tabindex="-1" href="@Url.Action("Danismanlar", "Home")">DANIŞMANLAR</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="#" class="dropdown-toggle" id="ddlmenuItem" data-toggle="dropdown">LİNKLER<i class="fa fa-angle-down"></i></a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="ddlmenuItem">
                                    <li role="presentation"><a role="menuitem" tabindex="-1" target="_blank" href="http://sorubank.ege.edu.tr/~moodle/mod/forum/view.php?id=735">Bölüm Forumu</a></li>
                                    <li role="presentation"><a role="menuitem" tabindex="-1" target="_blank" href="http://bilmuh.ege.edu.tr/microsoft_imagine">Microsoft Imagine Üyelikleri</a></li>
                                    <li role="presentation"><a role="menuitem" tabindex="-1" target="_blank" href="http://ebys.ege.edu.tr/ogrenci/ebp/tr/index.html">Bologna</a></li>
                                    <li role="presentation"><a role="menuitem" tabindex="-1" target="_blank" href="http://www.bmo.org.tr/">TMMOB Bilgisayar Mühendisleri Odası</a></li>
                                </ul>
                            </li>
                            <li> <a href="@Url.Action("Iletisim", "Home")" class="elements">İLETİŞİM </a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- MENU SECTION END-->
    <div class="content-wrapper">
        <div class="container">
            <div class="row pad-botm">
                <div class="col-md-12">
                    <div>
                        @RenderBody()
                    </div>
                </div>

            </div>
        </div>
    </div>
    <!-- CONTENT-WRAPPER SECTION END-->
    <section class="footer-section">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <a href="http://bilmuh.ege.edu.tr/" target="_blank">  Ege Üniversitesi Bilgisayar Mühendisliği Bölümü © @DateTime.Now.Year </a>
                </div>

            </div>
        </div>
    </section>
    <!-- FOOTER SECTION END-->
    <!-- JAVASCRIPT FILES PLACED AT THE BOTTOM TO REDUCE THE LOADING TIME  -->
    <!-- CORE JQUERY  -->
    <script src="~/Content/Base_Page/assets/js/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP SCRIPTS  -->
    <script src="~/Content/Base_Page/assets/js/bootstrap.js"></script>
    <!-- CUSTOM SCRIPTS  -->
    <script src="~/Content/Base_Page/assets/js/custom.js"></script>
</body>
</html>
